{
    "name": "tenet-ac-za/metadata-validator",
    "description": "SimpleSAMLphp module to record F-Ticks statistics",
    "type": "project",
    "keywords": ["saml"],
    "license": "MIT",
    "authors": [
        {
            "name": "Guy Halse",
            "homepage": "http://orcid.org/0000-0002-9388-8592"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-xml": "*",
        "ext-dom": "*",
        "ext-pcre": "*",
        "ext-json": "*",
        "jeremykendall/php-domain-parser": "^6.3"
    },
    "require-dev": {
        "vimeo/psalm": "^5.23",
        "slevomat/coding-standard": "^8.15"
    },
    "support": {
        "issues": "https://github.com/tenet-ac-za/metadata-validator/issues",
        "source": "https://github.com/tenet-ac-za/metadata-validator"
    }
}
